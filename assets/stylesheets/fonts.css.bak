@font-face {
  font-family: 'Gotham Bold';
  src: url('../fonts/Gotham\ Bold.eot');
  src:
    local('Gotham Bold'),
    local('Gotham-Bold'),
    url('../fonts/Gotham\ Bold.otf')
    format('opentype');
}

@font-face {
  font-family: 'Gotham Rounded Light';
  src: url('../fonts/Gotham\ Rounded\ Light.eot');
  src:
    local('Gotham Rounded Light'),
    local('Gotham-Rounded-Light'),
    url('../fonts/Gotham\ Rounded\ Light.otf')
    format('opentype');
}

